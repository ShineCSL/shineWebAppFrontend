#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -x "$basedir/node" ]; then
  "$basedir/node"  "$basedir/../generator-jhipster-nav-element/node_modules/generator-jhipster/node_modules/yeoman-generator/node_modules/semver/bin/semver" "$@"
  ret=$?
else 
  node  "$basedir/../generator-jhipster-nav-element/node_modules/generator-jhipster/node_modules/yeoman-generator/node_modules/semver/bin/semver" "$@"
  ret=$?
fi
exit $ret
